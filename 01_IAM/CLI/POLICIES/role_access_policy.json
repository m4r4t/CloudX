{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": ["s3:ListAllMyBuckets", "s3:GetBucketLocation"],
            "Effect": "Allow",
            "Resource": ["arn:aws:s3:::*"]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:Get*",
                "s3:List*"
            ],
            "Resource": [
                "arn:aws:s3:::iam-test-user1-6030/*",
                "arn:aws:s3:::iam-test-user1-6030"
            ]
        }
    ]
}
